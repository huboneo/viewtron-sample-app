{
  "name": "viewtron-sample-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf dist",
    "build:main": "tsc -p tsconfig.main.json",
    "build:ui": "NODE_ENV=development webpack --progress --colors",
    "watch:ui": "NODE_ENV=development webpack --watch --progress --colors",
    "lint:main": "tslint -c tslint.json -p tsconfig.main.json",
    "lint:ui": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run clean && npm run build:ui && concurrently -k \"npm:watch:ui\" \"npm:start:main\"",
    "start:main": "npm run build:main && electron ./dist/main/main.js"
  },
  "repository": "https://github.com/huboneo/viewtron-sample-app",
  "keywords": [
    "Viewtron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "huboneo",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "concurrently": "5.0.0",
    "css-loader": "3.2.0",
    "electron": "6.0.8",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "ts-loader": "6.2.1",
    "tslint": "5.19.0",
    "tslint-microsoft-contrib": "6.2.0",
    "typescript": "3.6.2",
    "viewtron": "2.2.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9"
  },
  "dependencies": {}
}
